@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix gly: <http://glyco.ai/schema#> .

gly:GlycanShape a sh:NodeShape ;
  sh:targetClass gly:Glycan ;
  sh:property [
    sh:path gly:hasMotif ;
    sh:datatype rdf:langString ;
    sh:minCount 0 ;
  ] .